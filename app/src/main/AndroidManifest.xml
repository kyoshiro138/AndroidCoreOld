<manifest package="com.jp.androidcore"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name=".appimplemetation.activity.AppFragmentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".appimplemetation.activity.drawer.AppDrawerActivity"/>

        <!-- Active Android Metadata-->
        <meta-data
            android:name="AA_DB_NAME"
            android:value="app.db"/>
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1"/>
        <meta-data
            android:name="AA_MODELS"
            android:value=""/>

    </application>

</manifest>
